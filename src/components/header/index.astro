---
import HeaderMenu, { Props as MenuProps } from "./menu/index.astro";
export interface Props {
  menu: MenuProps;
}
const { menu } = Astro.props;
---

<div class="z-30 duration-500 fixed top-0 left-0 w-full">
  <div
    class="pt-8 sm:pt-16 w-full flex justify-center bg-gradient-to-b from-dark to-primary"
  >
    <HeaderMenu {...menu} />
  </div>
  <div class="pt-12 bg-gradient-to-b from-primary to-opacity"></div>
</div>
